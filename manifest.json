{
    "manifest_version": 2,
    "name": "LATS Helper",
    "version": "1.2.0",
    "description": "Makes filling in your LATS timesheet and TDS easier",
    "content_scripts": [
        {
            "matches": [
                "https://oftlats.cma.com/*",
                "https://*.lats.ny.gov/*"
            ],
            "js": ["lats-helper.user.js"],
            "run_at": "document_end"
        }
    ],
    "permissions": [
        "storage"
    ]
}
